<div class="container">
  <h1>Vehicles First Entry Time</h1>

  <h2>Vehicle Details</h2>
  <table class='table table-dark table-striped-columns table-hover table-borderless align-middle table-responsive-sm'>
    <thead>
    <th>Vehicle Reg  No</th>
    <th>First Entry</th>
    </thead>
    <tbody>
    <% @vehicles.each do |vehicle|%>
      <tr>
        <td><%= vehicle.reg_no %></td>
        <td><%= vehicle.first_entry_time.localtime.strftime("%Y-%m-%d %H:%M:%S")%></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <div class="apple_pagination">
    <%= will_paginate @vehicles %>
  </div>
  <%= link_to 'Back', root_path, class: 'btn btn-outline-dark' %>
</div>